{
  "name": "firebase-mods",
  "version": "1.0.0",
  "description": "Repository of sample Firebase Mods.",
  "private": true,
  "scripts": {
    "format": "prettier --write {,**/}*.{yaml,js,md}",
    "test": "prettier --list-different {,**/}*.{yaml,js,md}",
    "extensions:clean": "lerna run clean",
    "extensions:build": "lerna run build",
    "extensions:test": "lerna run test",
    "postinstall": "lerna bootstrap && npm run extensions:clean && npm run extensions:build"
  },
  "repository": "",
  "author": "Firebase (https://firebase.google.com/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": ""
  },
  "devDependencies": {
    "lerna": "^3.4.3",
    "prettier": "1.15.3"
  }
}
