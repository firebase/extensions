{
  "name": "@firebaseextensions/firestore-translate-text",
  "version": "0.1.0",
  "description": "Translate an input text in a whole collection",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "github.com/firebase/extensions.git",
    "directory": "firestore-translate-text/scripts/translate-collection"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf ./lib",
    "compile": "tsc",
    "prepare": "npm run build",
    "test:local": "firebase ext:dev:emulators:exec ./node_modules/.bin/jest --test-params=./src/__tests__/emulator-params.env --project=dev-extensions-testing --config=./src/__tests__/firebase.json",
    "emulator:local": "firebase emulators:start --only firestore --project dev-extensions-testing --config ./src/__tests__/firebase.json",
    "test:watch": "jest --watch"
  },
  "files": [
    "lib"
  ],
  "bin": "./lib/index.js",
  "author": "Yaman KATBY <yaman@invertase.io>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/inquirer": "^9.0.1",
    "@types/jest": "^29.0.0",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@google-cloud/translate": "^7.0.1",
    "commander": "^9.4.0",
    "firebase-admin": "^11.0.1",
    "inquirer": "^6.5.2"
  }
}
