{
  "name": "delete-user-data-functions",
  "description": "Automatically delete a user's data when they delete their account.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "build:watch": "tsc --watch",
    "setup:emulator": "cd .. && firebase ext:dev:emulators:start --project=extensions-testing --test-params=test-params.env --test-config=./firebase.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:emulator": "firebase ext:dev:emulators:exec ./node_modules/.bin/jest --project=extensions-testing --test-params=../test-params.env --test-config=../firebase.json",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md"
  },
  "author": "Lauren Long <longlauren@google.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "firebase-admin": "9.12.0",
    "firebase-functions": "3.16.0",
    "firebase-tools": "9.23.0"
  },
  "devDependencies": {
    "jest": "27.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.0",
    "typescript": "4.3.5"
  },
  "private": true
}
