{
  "name": "auth-mailchimp-sync-functions",
  "description": "Add new users to a Mailchimp list, and delete them from the list when they delete their account.",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "test": "jest",
    "testIfEmulatorRunning": "wait-on tcp:4001 && jest",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md"
  },
  "dependencies": {
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^4.9.0",
    "mailchimp-api-v3": "^1.15.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "firebase-functions-test": "^3.2.0",
    "jest": "29.5.0",
    "ts-jest": "29.1.2",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": "18"
  },
  "author": "Lauren Long <longlauren@google.com>",
  "license": "Apache-2.0",
  "private": true
}
